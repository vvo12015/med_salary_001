<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet  id="25"  author="vvo12015">
        <dropTable tableName="user_position_user"/>
        <createTable  tableName="user_position_user">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column  name="user_id"  type="bigint">
                <constraints    foreignKeyName="fk_user_user_position_user"
                                referencedTableName="users"
                                referencedColumnNames="id"
                                deleteCascade="true"
                                nullable="false"/>
            </column>
            <column  name="user_position_id" type="bigint">
                <constraints    foreignKeyName="fk_user_position_user_position_user"
                                referencedTableName="user_position"
                                referencedColumnNames="id"
                                deleteCascade="true"
                                nullable="false"/>
            </column>
            <column name="employment" type="float" >
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>